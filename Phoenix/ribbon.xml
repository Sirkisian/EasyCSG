  <Application.Views>
    <Ribbon>
        <Ribbon.ApplicationMenu>
          <ApplicationMenu>
            <MenuGroup>
              <Button CommandName = "MenuNewProjectButton"/>
              <Button CommandName = "MenuOpenProjectButton"/>
              <Button CommandName = "MenuSaveAsProjectButton"/>
              <Button CommandName = "MenuSaveProjectButton"/>
            </MenuGroup>
            <MenuGroup>
               <Button CommandName = "MenuExitButton"/>
            </MenuGroup>
          </ApplicationMenu>
        </Ribbon.ApplicationMenu>
        <Ribbon.Tabs>
        <Tab CommandName = "ProjectTab">
          <Tab.ScalingPolicy>
            <ScalingPolicy>
              <ScalingPolicy.IdealSizes>
                <Scale Group = "ProjectGroup" Size = "Medium" />
              </ScalingPolicy.IdealSizes>
            </ScalingPolicy>
          </Tab.ScalingPolicy>

          <Group CommandName = "ProjectGroup" SizeDefinition = "FourButtons">
            <Button   CommandName = "NewProjectButton" />
            <Button   CommandName = "OpenProjectButton" />
            <Button   CommandName = "SaveAsProjectButton" />
            <Button   CommandName = "SaveProjectButton" />
          </Group>

          <Group CommandName = "ShortcutsGroup" SizeDefinition = "OneButton">
            <Button   CommandName = "ShowShortcutsButton" />
          </Group>

          <Group CommandName = "LogFileGroup" SizeDefinition = "OneButton">
            <Button   CommandName = "ShowLogFileButton" />
          </Group>
        </Tab>

        <Tab CommandName = "GraphicTab">

          <Group  CommandName = "ObjectGroup" SizeDefinition = "ButtonGroupsAndInputs">
            <SizeDefinition>
              <ControlNameMap>
                <ControlNameDefinition Name = "ObjectTypeGallery" />
                <ControlNameDefinition Name = "ObjectNameComboBox" />
                <ControlNameDefinition Name = "ObjectValuesComboBox" />
                <ControlNameDefinition Name = "ObjectAddButton" />
                <ControlNameDefinition Name = "ObjectCloneButton" />
                <ControlNameDefinition Name = "ObjectDeleteButton" />
              </ControlNameMap>
              <GroupSizeDefinition Size = "Large">
                <ControlSizeDefinition ControlName = "ObjectTypeGallery"    IsImageVisible = "true" IsLabelVisible = "true" ImageSize = "Large" />
                <ColumnBreak ShowSeparator = "true"/>
                <Row>
                  <ControlSizeDefinition ControlName = "ObjectNameComboBox"    IsImageVisible = "false" IsLabelVisible = "true" />
